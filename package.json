{
  "name": "@datenguide/datenguide-api",
  "description": "Datenguide API server",
  "version": "0.0.2-alpha.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Christian Rijke",
    "email": "hello@christianrijke.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datenguide/datenguide-api"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "build",
    "test": "test/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "lint": "eslint src/. --fix",
    "clean": "rm -rf ./build && mkdir build",
    "build": "npx babel -d ./build -D ./src",
    "start": "node ./build/index.js",
    "prepublishOnly": "yarn clean && yarn build",
    "pretty-quick": "npx pretty-quick --staged",
    "prettier": "npx prettier **/*.js --write",
    "dev": "babel-watch --watch src/templates src/index.js | npx pino-colada",
    "mocha": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@babel/cli": "7.5.5",
    "@feathersjs/configuration": "2.0.6",
    "@feathersjs/errors": "3.3.6",
    "@feathersjs/express": "1.3.1",
    "@feathersjs/feathers": "3.3.1",
    "apollo-cache-inmemory": "1.6.2",
    "apollo-client": "2.6.4",
    "apollo-link-schema": "1.2.3",
    "apollo-server-express": "2.8.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "elasticsearch": "16.3.0",
    "feathers-elasticsearch": "3.0.0",
    "feathers-envhelpers": "0.6.0",
    "feathers-hooks-common": "4.20.7",
    "feathers-logger": "0.3.2",
    "graphql": "14.4.2",
    "graphql-server-express": "1.4.1",
    "graphql-tools": "4.0.5",
    "graphql-type-json": "0.3.0",
    "helmet": "3.20.0",
    "json2csv": "4.5.2",
    "lodash": "4.17.15",
    "path": "0.12.7",
    "pino": "5.13.2",
    "pino-colada": "1.4.5",
    "quick-pivot": "2.7.0",
    "serve-favicon": "2.5.0",
    "sift": "8.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "babel-eslint": "10.0.2",
    "babel-watch": "7.0.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.15.0",
    "husky": "3.0.2",
    "mocha": "6.2.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "request": "2.88.0",
    "request-promise": "4.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 7
    },
    "parser": "babel-eslint",
    "globals": {
      "Teikei": true
    },
    "rules": {
      "no-extra-semi": 2,
      "semi": 0
    },
    "plugins": [
      "babel",
      "jest"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "yarn prettier"
    }
  }
}
