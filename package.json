{
  "name": "@datenguide/datenguide-api",
  "description": "Datenguide API server",
  "version": "0.0.2-alpha.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Christian Rijke",
    "email": "hello@christianrijke.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datenguide/datenguide-api"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "build",
    "test": "test/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "lint": "eslint src/. --fix",
    "clean": "rm -rf ./build && mkdir build",
    "build": "npx babel -d ./build -D ./src",
    "start": "node ./build/index.js",
    "prepublishOnly": "yarn clean && yarn build",
    "pretty-quick": "npx pretty-quick --staged",
    "prettier": "npx prettier **/*.js --write",
    "dev": "babel-watch --watch src/templates src/index.js | npx pino-colada",
    "mocha": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@babel/cli": "7.7.4",
    "@feathersjs/configuration": "4.3.11",
    "@feathersjs/errors": "4.3.11",
    "@feathersjs/express": "4.3.11",
    "@feathersjs/feathers": "4.3.11",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link-schema": "1.2.4",
    "apollo-server": "^2.9.12",
    "apollo-server-express": "2.9.12",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "8.2.0",
    "elasticsearch": "16.5.0",
    "feathers-elasticsearch": "3.1.0",
    "feathers-envhelpers": "0.6.2",
    "feathers-hooks-common": "4.20.7",
    "feathers-logger": "0.3.2",
    "file-system-cache": "^1.0.5",
    "genesis-online-js": "^0.0.3",
    "graphql": "14.5.8",
    "graphql-server-express": "1.4.1",
    "graphql-type-json": "0.3.1",
    "helmet": "3.21.2",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^24.9.0",
    "json2csv": "4.5.4",
    "lodash": "4.17.15",
    "lru-cache": "^5.1.1",
    "merge-graphql-schemas": "^1.7.3",
    "node-fetch": "^2.6.0",
    "path": "0.12.7",
    "pino": "5.14.0",
    "pino-colada": "1.5.0",
    "query-string": "^6.9.0",
    "quick-pivot": "2.7.0",
    "rimraf": "^3.0.0",
    "serve-favicon": "2.5.0",
    "sift": "9.0.0",
    "soap": "^0.30.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "apollo-server-testing": "^2.9.12",
    "babel-eslint": "10.0.3",
    "babel-watch": "7.0.0",
    "eslint": "6.7.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.4",
    "husky": "3.1.0",
    "mocha": "6.2.2",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "request": "2.88.0",
    "request-promise": "4.2.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 7
    },
    "parser": "babel-eslint",
    "globals": {
      "Teikei": true
    },
    "rules": {
      "no-extra-semi": 2,
      "semi": 0
    },
    "plugins": [
      "babel",
      "jest"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "yarn prettier"
    }
  }
}
