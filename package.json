{
  "name": "@datenguide/datenguide-api",
  "description": "Datenguide API server",
  "version": "0.0.2-alpha.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Christian Rijke",
    "email": "hello@christianrijke.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datenguide/datenguide-api"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "build",
    "test": "test/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "lint": "eslint src/. --fix",
    "clean": "rm -rf ./build && mkdir build",
    "build": "npx babel -d ./build -D ./src",
    "start": "node ./build/index.js",
    "prepublishOnly": "yarn clean && yarn build",
    "pretty-quick": "npx pretty-quick --staged",
    "prettier": "npx prettier **/*.js --write",
    "dev": "babel-watch --watch src/templates src/index.js | npx pino-colada",
    "mocha": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@babel/cli": "7.8.4",
    "@feathersjs/configuration": "4.5.2",
    "@feathersjs/errors": "4.5.2",
    "@feathersjs/express": "4.5.2",
    "@feathersjs/feathers": "4.5.2",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link-schema": "1.2.4",
    "apollo-server": "^2.11.0",
    "apollo-server-express": "2.11.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "8.2.0",
    "elasticsearch": "16.7.2",
    "feathers-elasticsearch": "3.1.0",
    "feathers-envhelpers": "0.6.2",
    "feathers-hooks-common": "5.0.2",
    "feathers-logger": "0.3.2",
    "file-system-cache": "^1.0.5",
    "genesis-online-js": "^0.0.3",
    "graphql": "14.6.0",
    "graphql-type-json": "0.3.1",
    "helmet": "3.22.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^25.2.3",
    "json2csv": "5.0.0",
    "lodash": "4.17.15",
    "lru-cache": "^5.1.1",
    "merge-graphql-schemas": "^1.7.6",
    "node-fetch": "^2.6.0",
    "path": "0.12.7",
    "pino": "5.17.0",
    "pino-colada": "2.0.0",
    "query-string": "^6.11.1",
    "quick-pivot": "2.7.0",
    "rimraf": "^3.0.2",
    "serve-favicon": "2.5.0",
    "sift": "12.0.0",
    "soap": "^0.31.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "apollo-server-testing": "^2.11.0",
    "babel-eslint": "10.1.0",
    "babel-watch": "7.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "4.2.3",
    "mocha": "7.1.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "2.0.2",
    "pretty-quick": "2.0.1",
    "request": "2.88.2",
    "request-promise": "4.2.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    },
    "env": {
      "jest/globals": true,
      "node": true
    },
    "plugins": [
      "jest"
    ],
    "extends": [
      "standard",
      "prettier"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "yarn pretty-quick"
    }
  }
}
